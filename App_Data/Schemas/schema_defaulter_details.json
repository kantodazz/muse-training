{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType"
          ]
        },
        "messageSummary": {
          "type": "object",
          "properties": {
            "createdAt": {
              "type": "string"
            },
            "orgMsgId": {
              "type": "string"
            },
            "orgMessageType": {
              "type": "string"
            }
          },
          "required": [
            "createdAt",
            "orgMsgId",
            "orgMessageType"
          ]
        },
        "messageDetails": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "controlNumber": {
                  "type": "string"
                },
                "agencyNoticeRefNo": {
                  "type": "string"
                },
                "amount": {
                  "type": "number"
                },
                "agencyNoticeFile": {
                  "type": "string"
                }
              },
              "required": [
                "controlNumber",
                "agencyNoticeRefNo",
                "amount",
                "agencyNoticeFile"
              ]
            }
          ]
        }
      },
      "required": [
        "messageHeader",
        "messageSummary",
        "messageDetails"
      ]
    }
  },
  "required": [
    "message"
  ]
}