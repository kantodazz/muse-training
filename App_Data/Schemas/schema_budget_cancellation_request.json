{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "paymentType": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType",
            "paymentType"
          ]
        },
        "messageSummary": {
          "type": "object",
          "properties": {
            "createdAt": {
              "type": "string"
            }
          },
          "required": [
            "createdAt"
          ]
        },
        "messageDetails": {
          "type": "object",
          "properties": {
            "orgReferenceNo": {
              "type": "string"
            },
            "status": {
              "type": "string"
            },
            "statusDesc": {
              "type": "string"
            },
            "endToEndId": {
              "type": "string"
            },
            "attachments": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "attachmentName": {
                      "type": "string"
                    },
                    "attachmentData": {
                      "type": "string"
                    },
                    "attachmentUrl": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "attachmentName",
                    "attachmentData",
                    "attachmentUrl"
                  ]
                }
              ]
            }
          },
          "required": [
            "orgReferenceNo",
            "status",
            "statusDesc",
            "endToEndId",
            "attachments"
          ]
        }
      },
      "required": [
        "messageHeader",
        "messageSummary",
        "messageDetails"
      ]
    },
    "digitalSignature": {
      "type": "string"
    }
  },
  "required": [
    "message",
    "digitalSignature"
  ]
}