{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "paymentType": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType",
            "paymentType"
          ]
        },
        "messageSummary": {
          "type": "object",
          "properties": {
            "financialYear": {
              "type": "string"
            },
            "institutionCode": {
              "type": "string"
            },
            "institutionName": {
              "type": "string"
            },
            "transactionType": {
              "type": "string"
            },
            "gfsCode": {
              "type": "string"
            },
            "gfsCodeDesc": {
              "type": "string"
            },
            "baseCurrency": {
              "type": "string"
            },
            "baseAmount": {
              "type": "number"
            },
            "tbAmount": {
              "type": "number"
            },
            "numTrx": {
              "type": "integer"
            },
            "totalEliminationAmount": {
              "type": "number"
            },
            "totalCounterpartAmount": {
              "type": "number"
            },
            "createdBy": {
              "type": "string"
            },
            "createdAt": {
              "type": "string"
            },
            "submittedAt": {
              "type": "string"
            },
            "submittedBy": {
              "type": "string"
            }
          },
          "required": [
            "financialYear",
            "institutionCode",
            "institutionName",
            "transactionType",
            "gfsCode",
            "gfsCodeDesc",
            "baseCurrency",
            "baseAmount",
            "tbAmount",
            "numTrx",
            "totalEliminationAmount",
            "totalCounterpartAmount",
            "createdBy",
            "createdAt",
            "submittedAt",
            "submittedBy"
          ]
        },
        "messageDetails": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "fiscalYear": {
                "type": "string"
              },
              "consoCounterpartGfsCode": {
                "type": "string"
              },
              "consoCounterpartGfsDesc": {
                "type": "string"
              },
              "consoCounterpartTBAmt": {
                "type": "number"
              },
              "eliminatingEntityAmount": {
                "type": "number"
              },
              "consoCounterpartInstCode": {
                "type": "string"
              },
              "consoCounterpartEntityDescription": {
                "type": "string"
              }
            },
            "required": [
              "fiscalYear",
              "consoCounterpartGfsCode",
              "consoCounterpartGfsDesc",
              "consoCounterpartTBAmt",
              "eliminatingEntityAmount",
              "consoCounterpartInstCode",
              "consoCounterpartEntityDescription"
            ]
          }
        }
      },
      "required": [
        "messageHeader",
        "messageSummary",
        "messageDetails"
      ]
    },
    "digitalSignature": {
      "type": "string"
    }
  },
  "required": [
    "message",
    "digitalSignature"
  ]
}