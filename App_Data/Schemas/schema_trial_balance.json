{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "paymentType": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType",
            "paymentType"
          ]
        },
        "messageSummary": {
          "type": "object",
          "properties": {
            "createdAt": {
              "type": "string"
            },
            "totalTransaction": {
              "type": "integer"
            },
            "institutionCode": {
              "type": "string"
            },
            "institutionName": {
              "type": "string"
            },
            "financialYear": {
              "type": "string"
            },
            "checkDate": {
              "type": "string"
            }
          },
          "required": [
            "createdAt",
            "totalTransaction",
            "institutionCode",
            "institutionName",
            "financialYear",
            "checkDate"
          ]
        },
        "messageDetails": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "gfsCode": {
                "type": "string"
              },
              "transactionType": {
                "type": "string"
              },
              "baseCurrency": {
                "type": "string"
              },
              "baseAmount": {
                "type": "number"
              }
            },
            "required": [
              "gfsCode",
              "transactionType",
              "baseCurrency",
              "baseAmount"
            ]
          }
        }
      },
      "required": [
        "messageHeader",
        "messageSummary",
        "messageDetails"
      ]
    },
    "digitalSignature": {
      "type": "string"
    }
  },
  "required": [
    "message",
    "digitalSignature"
  ]
}