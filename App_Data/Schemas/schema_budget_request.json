{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "esbBody": {
          "type": "object",
          "properties": {
            "message": {
              "type": "object",
              "properties": {
                "messageSummary": {
                  "type": "object",
                  "properties": {
                    "financialYear": {
                      "type": "string"
                    },
                    "createdAt": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "financialYear",
                    "createdAt"
                  ]
                },
                "messageHeader": {
                  "type": "object",
                  "properties": {
                    "receiver": {
                      "type": "string"
                    },
                    "messageType": {
                      "type": "string"
                    },
                    "sender": {
                      "type": "string"
                    },
                    "msgId": {
                      "type": "string"
                    },
                    "paymentType": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "receiver",
                    "messageType",
                    "sender",
                    "msgId",
                    "paymentType"
                  ]
                },
                "messageDetails": {
                  "type": "array",
                  "items": [
                    {
                      "type": "string"
                    }
                  ]
                },
                "digitalSignature": {
                  "type": "string"
                }
              },
              "required": [
                "messageSummary",
                "messageHeader",
                "messageDetails",
                "digitalSignature"
              ]
            }
          },
          "required": [
            "message"
          ]
        },
        "requestId": {
          "type": "string"
        }
      },
      "required": [
        "esbBody",
        "requestId"
      ]
    },
    "signature": {
      "type": "string"
    }
  },
  "required": [
    "data",
    "signature"
  ]
}