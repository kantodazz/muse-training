{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "digitalSignature": {
      "type": "string"
    },
    "message": {
      "type": "object",
      "properties": {
        "messageSummary": {
          "type": "object",
          "properties": {
            "totalAmount": {
              "type": "number"
            },
            "exchequerRef": {
              "type": "string"
            },
            "noOfTrxs": {
              "type": "integer"
            },
            "subBudgetClass": {
              "type": "string"
            },
            "applyDate": {
              "type": "string"
            },
            "sourceModule": {
              "type": "string"
            }
          },
          "required": [
            "totalAmount",
            "exchequerRef",
            "noOfTrxs",
            "subBudgetClass",
            "applyDate",
            "sourceModule"
          ]
        },
        "messageHeader": {
          "type": "object",
          "properties": {
            "createdAt": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "sender": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            }
          },
          "required": [
            "createdAt",
            "receiver",
            "messageType",
            "sender",
            "msgId"
          ]
        },
        "messageDetails": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "uid": {
                  "type": "string"
                },
                "voteName": {
                  "type": "string"
                },
                "amount": {
                  "type": "number"
                },
                "projectCode": {
                  "type": "string"
                },
                "voteCode": {
                  "type": "string"
                },
                "componentDescription": {
                  "type": "string"
                },
                "componentName": {
                  "type": "string"
                },
                "mainVoteCode": {
                  "type": "string"
                },
                "orgUid": {
                  "type": "string"
                },
                "projectName": {
                  "type": "string"
                },
                "mainVoteName": {
                  "type": "string"
                }
              },
              "required": [
                "uid",
                "voteName",
                "amount",
                "projectCode",
                "voteCode",
                "componentDescription",
                "componentName",
                "mainVoteCode",
                "orgUid",
                "projectName",
                "mainVoteName"
              ]
            }
          ]
        }
      },
      "required": [
        "messageSummary",
        "messageHeader",
        "messageDetails"
      ]
    }
  },
  "required": [
    "digitalSignature",
    "message"
  ]
}