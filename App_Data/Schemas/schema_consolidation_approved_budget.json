{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "paymentType": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType",
            "paymentType"
          ]
        },
        "messageSummary": {
          "type": "object",
          "properties": {
            "createdAt": {
              "type": "string"
            },
            "institutionCode": {
              "type": "string"
            },
            "institutionName": {
              "type": "string"
            },
            "financialYear": {
              "type": "string"
            },
            "applyDate": {
              "type": "string"
            }
          },
          "required": [
            "createdAt",
            "institutionCode",
            "institutionName",
            "financialYear",
            "applyDate"
          ]
        },
        "messageDetails": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "subBudgetClass": {
                "type": "string",
                "minLength": 3,
                "maxLength": 3
              },
              "subBudgetClassDesc": {
                "type": "string"
              },
              "baseCurrency": {
                "type": "string"
              },
              "journalTypeCode": {
                "type": "string"
              },
              "baseAmount": {
                "type": "number"
              },
              "gfsCode": {
                "type": "string"
              },
              "gfsDescription": {
                "type": "string"
              },
              "createdAt": {
                "type": "string"
              },
              "createdBy": {
                "type": "string"
              },
              "submittedBy": {
                "type": "string"
              },
              "submissionDate": {
                "type": "string"
              }
            },
            "required": [
              "subBudgetClass",
              "subBudgetClassDesc",
              "baseCurrency",
              "journalTypeCode",
              "baseAmount",
              "gfsCode",
              "gfsDescription",
              "createdAt",
              "createdBy",
              "submittedBy",
              "submissionDate"
            ]
          }
        }
      },
      "required": [
        "messageHeader",
        "messageSummary",
        "messageDetails"
      ]
    },
    "digitalSignature": {
      "type": "string"
    }
  },
  "required": [
    "message",
    "digitalSignature"
  ]
}