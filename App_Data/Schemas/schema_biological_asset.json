{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "paymentType": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType",
            "paymentType"
          ]
        },
        "messageSummary": {
          "type": "object",
          "properties": {
            "institutionCode": {
              "type": "string"
            },
            "institutionName": {
              "type": "string"
            },
            "financialYear": {
              "type": "string"
            },
            "checkDate": {
              "type": "string"
            },
            "createdAt": {
              "type": "string"
            },
            "createdBy": {
              "type": "string"
            },
            "submittedBy": {
              "type": "string"
            },
            "submissionDate": {
              "type": "string"
            }
          },
          "required": [
            "institutionCode",
            "institutionName",
            "financialYear",
            "checkDate",
            "createdAt",
            "createdBy",
            "submittedBy",
            "submissionDate"
          ]
        },
        "messageDetails": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "gfsCode": {
                "type": "string"
              },
              "gfsDescription": {
                "type": "string"
              },
              "openingAmount": {
                "type": "number"
              },
              "fairValueAmount": {
                "type": "number"
              },
              "valuePhysicalAmount": {
                "type": "number"
              },
              "regenerationIncreaseAmount": {
                "type": "number"
              },
              "salesDecreaseAmount": {
                "type": "number"
              },
              "otherChangesAmount": {
                "type": "number"
              },
              "totalAmount": {
                "type": "number"
              }
            },
            "required": [
              "gfsCode",
              "gfsDescription",
              "openingAmount",
              "fairValueAmount",
              "valuePhysicalAmount",
              "regenerationIncreaseAmount",
              "salesDecreaseAmount",
              "otherChangesAmount",
              "totalAmount"
            ]
          }
        }
      },
      "required": [
        "messageHeader",
        "messageSummary",
        "messageDetails"
      ]
    },
    "digitalSignature": {
      "type": "string"
    }
  },
  "required": [
    "message",
    "digitalSignature"
  ]
}