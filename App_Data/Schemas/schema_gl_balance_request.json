{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "esbBody": {
          "type": "object",
          "properties": {
            "messageHeader": {
              "type": "object",
              "properties": {
                "sender": {
                  "type": "string"
                },
                "receiver": {
                  "type": "string"
                },
                "msgId": {
                  "type": "string"
                },
                "messageType": {
                  "type": "string"
                },
                "paymentType": {
                  "type": "string"
                }
              },
              "required": [
                "sender",
                "receiver",
                "msgId",
                "messageType",
                "paymentType"
              ]
            },
            "messageSummary": {
              "type": "object",
              "properties": {
                "createdAt": {
                  "type": "string"
                },
                "financialYear": {
                  "type": "string"
                }
              },
              "required": [
                "createdAt",
                "financialYear"
              ]
            },
            "messageDetails": {
              "type": "array",
              "items": [
                {
                  "type": "string"
                },
                {
                  "type": "string"
                }
              ]
            }
          },
          "required": [
            "messageHeader",
            "messageSummary",
            "messageDetails"
          ]
        },
        "requestId": {
          "type": "string"
        }
      },
      "required": [
        "esbBody",
        "requestId"
      ]
    },
    "signature": {
      "type": "string"
    }
  },
  "required": [
    "data",
    "signature"
  ]
}