{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "paymentType": {
              "type": "string"
            },
            "createdAt": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType",
            "paymentType",
            "createdAt"
          ]
        },
        "paymentSummary": {
          "type": "object",
          "properties": {
            "institutioncode": {
              "type": "string"
            },
            "referenceNo": {
              "type": "string"
            },
            "paymentDesc": {
              "type": "string"
            },
            "currencyCode": {
              "type": "string"
            },
            "applyDate": {
              "type": "string"
            },
            "totalAmount": {
              "type": "number"
            },
            "noofTransaction": {
              "type": "integer"
            },
            "isSTP": {
              "type": "string"
            }
          },
          "required": [
            "institutioncode",
            "referenceNo",
            "paymentDesc",
            "currencyCode",
            "applyDate",
            "totalAmount",
            "noofTransaction",
            "isSTP"
          ]
        },
        "payList": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "payeeCode": {
                  "type": "string"
                },
                "payeeName": {
                  "type": "string"
                },
                "payeeAccountNumber": {
                  "type": "string"
                },
                "payeeAccountName": {
                  "type": "string"
                },
                "payeeBankName": {
                  "type": "string"
                },
                "payeeBankBic": {
                  "type": "string"
                },
                "amount": {
                  "type": "number"
                },
                "endtoEndId": {
                  "type": "string"
                },
                "paymentChannel": {
                  "type": "string"
                }
              },
              "required": [
                "payeeCode",
                "payeeName",
                "payeeAccountNumber",
                "payeeAccountName",
                "payeeBankName",
                "payeeBankBic",
                "amount",
                "endtoEndId",
                "paymentChannel"
              ]
            },
            {
              "type": "object",
              "properties": {
                "payeeCode": {
                  "type": "string"
                },
                "payeeName": {
                  "type": "string"
                },
                "payeeAccountNumber": {
                  "type": "string"
                },
                "payeeAccountName": {
                  "type": "string"
                },
                "payeeBankName": {
                  "type": "string"
                },
                "payeeBankBic": {
                  "type": "string"
                },
                "amount": {
                  "type": "number"
                },
                "endtoEndId": {
                  "type": "string"
                },
                "paymentChannel": {
                  "type": "string"
                }
              },
              "required": [
                "payeeCode",
                "payeeName",
                "payeeAccountNumber",
                "payeeAccountName",
                "payeeBankName",
                "payeeBankBic",
                "amount",
                "endtoEndId",
                "paymentChannel"
              ]
            }
          ]
        }
      },
      "required": [
        "messageHeader",
        "paymentSummary",
        "payList"
      ]
    },
    "digitalSignature": {
      "type": "string"
    }
  },
  "required": [
    "message",
    "digitalSignature"
  ]
}