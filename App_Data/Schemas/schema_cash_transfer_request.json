{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "digitalSignature": {
      "type": "string"
    },
    "message": {
      "type": "object",
      "properties": {
        "messageSummary": {
          "type": "object",
          "properties": {
            "bankName": {
              "type": "string"
            },
            "bankBIC": {
              "type": "string"
            },
            "amount": {
              "type": "string"
            },
            "documentNo": {
              "type": "string"
            },
            "fundCategory": {
              "type": "string"
            },
            "payStationId": {
              "type": "string",
              "pattern": "^[0-9]{1,5}$"
            },
            "bankAccountNo": {
              "type": "string"
            },
            "attachment": {
              "type": [ "string", "null" ]
            },
            "bankAccountName": {
              "type": "string"
            },
            "receiptType": {
              "type": "string"
            },
            "sbc": {
              "type": "string"
            },
            "fundType": {
              "type": "string"
            },
            "currency": {
              "type": "string"
            },
            "company": {
              "type": "string"
            },
            "customerName": {
              "type": "string"
            },
            "applyDate": {
              "type": "string"
            },
            "referenceNo": {
              "type": "string"
            },
            "receiptDate": {
              "type": "string"
            }
          },
          "required": [
            "bankName",
            "bankBIC",
            "amount",
            "documentNo",
            "fundCategory",
            "payStationId",
            "bankAccountNo",
            "attachment",
            "bankAccountName",
            "receiptType",
            "sbc",
            "fundType",
            "currency",
            "company",
            "customerName",
            "applyDate",
            "referenceNo",
            "receiptDate"
          ]
        },
        "messageHeader": {
          "type": "object",
          "properties": {
            "createdAt": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "sender": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            }
          },
          "required": [
            "createdAt",
            "receiver",
            "messageType",
            "sender",
            "msgId"
          ]
        },
        "messageDetails": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "sbcTo": {
                "type": "string"
              },
              "uid": {
                "type": "string"
              },
              "description": {
                "type": "string"
              },
              "institutionTo": {
                "type": "string"
              },
              "bankAccountTo": {
                "type": "string"
              },
              "amount": {
                "type": "string"
              },
              "transferCategory": {
                "type": "string"
              },
              "transferType": {
                "type": "string"
              }
            },
            "required": [
              "sbcTo",
              "uid",
              "description",
              "institutionTo",
              "bankAccountTo",
              "amount",
              "transferCategory",
              "transferType"
            ]
          }
        }
      },
      "required": [
        "messageSummary",
        "messageHeader",
        "messageDetails"
      ]
    }
  },
  "required": [
    "digitalSignature",
    "message"
  ]
}