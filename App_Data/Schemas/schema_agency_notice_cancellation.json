{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType"
          ]
        },
        "messageSummary": {
          "type": "object",
          "properties": {
            "createdAt": {
              "type": "string"
            },
            "controlNo": {
              "type": "string"
            },
            "remarks": {
              "type": "string"
            },
            "agencyNoticeRefNo": {
              "type": "string"
            },
            "cancelNoticeRefNo": {
              "type": "string"
            },
            "cancelNoticeFile": {
              "type": "string"
            }
          },
          "required": [
            "createdAt",
            "controlNo",
            "remarks",
            "agencyNoticeRefNo",
            "cancelNoticeRefNo",
            "cancelNoticeFile"
          ]
        }
      },
      "required": [
        "messageHeader",
        "messageSummary"
      ]
    }
  },
  "required": [
    "message"
  ]
}