{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "createdAt": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType",
            "createdAt"
          ]
        },
        "messageSummary": {
          "type": "object",
          "properties": {
            "institutionCode": {
              "type": "string"
            },
            "institutionName": {
              "type": "string"
            },
            "subInstitutionCode": {
              "type": "string"
            },
            "subInstitutionName": {
              "type": "string"
            },
            "dateFrom": {
              "type": "string"
            },
            "dateTo": {
              "type": "string"
            }
          },
          "required": [
            "institutionCode",
            "institutionName",
            "subInstitutionCode",
            "subInstitutionName",
            "dateFrom",
            "dateTo"
          ]
        }
      },
      "required": [
        "messageHeader",
        "messageSummary"
      ]
    },
    "digitalSignature": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "required": [
    "message",
    "digitalSignature"
  ]
}