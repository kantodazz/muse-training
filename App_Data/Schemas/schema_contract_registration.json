{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageSummary": {
          "type": "object",
          "properties": {
            "createdAt": {
              "type": "string"
            }
          },
          "required": [
            "createdAt"
          ]
        },
        "messageHeader": {
          "type": "object",
          "properties": {
            "receiver": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "sender": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "paymentType": {
              "type": "string"
            }
          },
          "required": [
            "receiver",
            "messageType",
            "sender",
            "msgId",
            "paymentType"
          ]
        },
        "messageDetails": {
          "type": "object",
          "properties": {
            "referenceNo": {
              "type": "string"
            },
            "institutionCode": {
              "type": "string"
            },
            "contractType": {
              "type": "string"
            },
            "institutionName": {
              "type": "string"
            },
            "departmentCode": {
              "type": "string"
            },
            "departmentName": {
              "type": "string"
            },
            "payeeCode": {
              "type": "string"
            },
            "payeeName": {
              "type": "string"
            },
            "payeeBankAccount": {
              "type": "string"
            },
            "payeeBankName": {
              "type": "string"
            },
            "payeeAccountName": {
              "type": "string"
            },
            "payeeBic": {
              "type": "string"
            },
            "payeeAddress": {
              "type": "string"
            },
            "payeeType": {
              "type": "string"
            },
            "subBudgetClass": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "amount": {
              "type": "number"
            },
            "vatAmount": {
              "type": [ "number", "null" ]
            },
            "currency": {
              "type": "string"
            },
            "startDate": {
              "type": "string"
            },
            "endDate": {
              "type": "string"
            },
            "hasAdvancePayment": {
              "type": [ "boolean", "null" ]
            },
            "advancePaymentPtg": {
              "type": [ "number", "null" ]
            },
            "hasRetention": {
              "type": [ "boolean", "null" ]
            },
            "retentionPtg": {
              "type": [ "number", "null" ]
            },
            "retentionPtgPerCertificate": {
              "type": [ "number", "null" ]
            },
            "liquidatedDamagePtg": {
              "type": [ "number", "null" ]
            },
            "parentInstitutionCode": {
              "type": [ "string", "null" ]
            },
            "isForSubWarant": {
              "type": [ "boolean", "null" ]
            },
            "subWarantCode": {
              "type": [ "string", "null" ]
            },
            "glAccounts": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "glAccount": {
                    "type": "string"
                  },
                  "amount": {
                    "type": "number"
                  }
                },
                "required": [
                  "glAccount",
                  "amount"
                ]
              }
            },
            "attachments": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "attachmentName": {
                      "type": "string"
                    },
                    "attachmentData": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "attachmentName",
                    "attachmentData"
                  ]
                }
              ]
            }
          },
          "required": [
            "referenceNo",
            "institutionCode",
            "contractType",
            "institutionName",
            "departmentCode",
            "departmentName",
            "payeeCode",
            "payeeName",
            "payeeBankAccount",
            "payeeBankName",
            "payeeAccountName",
            "payeeBic",
            "payeeAddress",
            "payeeType",
            "subBudgetClass",
            "description",
            "amount",
            "vatAmount",
            "currency",
            "startDate",
            "endDate",
            "isForSubWarant",
            "glAccounts",
            "attachments"
          ]
        }
      },
      "required": [
        "messageSummary",
        "messageHeader",
        "messageDetails"
      ]
    },
    "digitalSignature": {
      "type": "string"
    }
  },
  "required": [
    "message",
    "digitalSignature"
  ]
}