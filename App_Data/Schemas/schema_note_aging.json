{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "object",
      "properties": {
        "messageHeader": {
          "type": "object",
          "properties": {
            "sender": {
              "type": "string"
            },
            "receiver": {
              "type": "string"
            },
            "msgId": {
              "type": "string"
            },
            "messageType": {
              "type": "string"
            },
            "paymentType": {
              "type": "string"
            }
          },
          "required": [
            "sender",
            "receiver",
            "msgId",
            "messageType",
            "paymentType"
          ]
        },
        "messageSummary": {
          "type": "object",
          "properties": {
            "institutionCode": {
              "type": "string"
            },
            "institutionName": {
              "type": "string"
            },
            "disclosureReference": {
              "type": "string"
            },
            "disclosureCategory": {
              "type": "string"
            },
            "financialYear": {
              "type": "string"
            },
            "checkDate": {
              "type": "string"
            },
            "createdAt": {
              "type": "string"
            },
            "createdBy": {
              "type": "string"
            },
            "submittedBy": {
              "type": "string"
            },
            "submissionDate": {
              "type": "string"
            }
          },
          "required": [
            "institutionCode",
            "institutionName",
            "disclosureReference",
            "disclosureCategory",
            "financialYear",
            "checkDate",
            "createdAt",
            "createdBy",
            "submittedBy",
            "submissionDate"
          ]
        },
        "messageDetails": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "gfsCode": {
                "type": "string"
              },
              "gfsCodeDescription": {
                "type": "string"
              },
              "tbAmount": {
                "type": "number"
              },
              "upTo1Month": {
                "type": "number"
              },
              "c1To3Months": {
                "type": "number"
              },
              "c3To12Months": {
                "type": "number"
              },
              "c1To3Years": {
                "type": "number"
              },
              "c3To5Years": {
                "type": "number"
              },
              "over5Years": {
                "type": "number"
              }
            },
            "required": [
              "gfsCode",
              "gfsCodeDescription",
              "tbAmount",
              "upTo1Month",
              "c1To3Months",
              "c3To12Months",
              "c1To3Years",
              "c3To5Years",
              "over5Years"
            ]
          }
        }
      },
      "required": [
        "messageHeader",
        "messageSummary",
        "messageDetails"
      ]
    },
    "digitalSignature": {
      "type": "string"
    }
  },
  "required": [
    "message",
    "digitalSignature"
  ]
}