@section pagespecific {
    <!-- PAGE RELATED PLUGIN(S) -->
    <script type="text/javascript">

        $(document).ready(function () {
            var auditorId = $("#AuditorId").val();
            updateWorkingZone(auditorId);
            updateProfessional(auditorId);
        });

        var dt_workingZones = $('#dt_workingZones').dataTable();
        $("#dt_workingZones_wrapper .dt-toolbar").remove();
        $("#dt_workingZones_wrapper .dt-toolbar-footer").remove();

        var dt_professional = $('#dt_professional').dataTable();
        $("#dt_professional_wrapper .dt-toolbar").remove();
        $("#dt_professional_wrapper .dt-toolbar-footer").remove();

        function ChoseProfessional() {
            var id = $("#AuditorId").val();
            $(".modal-title").text("Update Auditor Professional");
            $("#btnSaveProfessional").show();
            $("#UpdateProfessional").modal("show");
            updateProfessional(id);
        }

        function updateProfessional(pid)
        {
            /*debugger*/;
            $("#divLoader").show();
            var url = '@Url.Action("GetAuditorProfessionalList", "Auditor")';
            var params = { "id": pid };

            $.ajax({
                type: "get",
                url: url,
                data: params,
                contenttype: "application/json; charset=utf-8",
                datatype: "json",
                success: function (response) {
                    $("#divLoader").hide();
                    data = response.data;
                    datalength = data.length;
                    dt_professional.fnClearTable();
                    for (var i = 0; i < data.length; i++) {
                        dt_professional.fnAddData([i+1,
                            data[i]["ProfessionalName"],
                            '<a href="#" onclick="ProfessionalRemove(' + i + ',' + data[i]["Id"] + ')">\
                           <i class="glyphicon glyphicon-trash"></i></a>'
                        ]);
                    }
                    $("#totalProfessional").text("(" + data.length + ")");
                    countProfessional =  data.length;
                },
                failure: function (error) {
                    swal(error);
                }
            });
        }

        var allprofessional = []
        var professional = []
        var countProfessional = 0;

        function addProfessional() {
            //debugger;
            $("#addProfessional").prop('disabled', true);
            var pId = $('#SelectedProfessional').val();
            var auditorId = $("#AuditorId").val();
            var pName = $('#SelectedProfessional option:selected').text();


            if (!professional.includes(pId)) {
                $("#divLoader").show();
                var formData = new FormData();
                formData.append('__RequestVerificationToken', $('[name=__RequestVerificationToken]').val());
                formData.append('ProfessionalId', $("#SelectedProfessional").val());
                formData.append('AuditorId', auditorId);
                formData.append('ProfessionalName', pName);

                var url = '@Url.Action("AddAuditorProfessional", "Auditor")';
                $.ajax({
                    type: "POST",
                    url: url,
                    data: formData,
                    contentType: false,
                    processData: false,
                    success: function (response) {
                        $("#addProfessional").prop('disabled', false);
                        $("#divLoader").hide();
                        if (response == "Success") {
                            swal("Professional Updated Successfuly!", { icon: "success" });
                            $("#addProfessional").prop('disabled', false);
                            allprofessional.push({
                                FileName: $('#SelectedProfessional option:selected').text()
                            });
                            professional.push($('#SelectedProfessional option:selected').text());
                            updateProfessional(auditorId);
                        } else if (response == "Dublicate") {
                            swal("Professional alread registered!", { icon: "warning" })
                        }
                        else {
                            swal("Error in Add Professional!", { icon: "warning" })
                         }
                      },
                    failure: function (response) {
                        alert(response.d);
                    }
                });

            }
            else {
                    swal("Region Details required");
            }

        }


         function ProfessionalRemove(index,id) {
            var params = {"id":  id };
             var id = $("#AuditorId").val();
            var url = '@Url.Action("RemoveAuditorProfessional", "Auditor")';
            $.ajax({
                type: "get",
                url: url,
                data: params,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                    success: function (response) {
                        if (response = "Success") {
                            swal("Remove Successfully!", { icon: "success" })
                                .then((value) => {
                                    allprofessional.splice(index, 1);
                                    professional.splice(index, 1);
                                    updateProfessional(id);
                                });
                        }
                    },
                    failure: function (response) {
                        alert(response.d);
                    }
            });
         }

        function ChoseLocation() {
            var id = $("#AuditorId").val();
            $(".modal-title").text("Update Working Zone");
            $("#btnSaveWorkingZone").show();
            $("#UpdateZone").modal("show");
            updateWorkingZone(id);
        }

