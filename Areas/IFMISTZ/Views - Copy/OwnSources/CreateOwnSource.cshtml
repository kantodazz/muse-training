                                                <td style="text-align:left">@group1.RelatedRef</td>
                                                <td style="text-align:left">@group1.Reference</td>
                                                <td style="text-align:left">@group1.OperationalAmount</td>
                                                <td style="text-align:left">@group1.Description</td>
                                                <td style="text-align:left;white-space:nowrap">@group1.GlAccount</td>
