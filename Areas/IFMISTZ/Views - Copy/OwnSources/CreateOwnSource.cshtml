            });
            $('#confirmModal').modal('hide');
        }



        function loadOwnSource() {
            $.ajax({
                type: "post",
                url: '@Url.Action("LoadOwnSource", "OwnSources")',
                contenttype: "application/json; charset=utf-8",
                datatype: "json",
                success: function (response) {
                    if (response == "Success") {
                        swal("Items Loaded Successfully..");
                        window.location.reload();
                    }
                    else {
                        swal(response);
                    }
                },
                failure: function (error) {
                    swal(error);
                }
            });
        }



        var viewReason = function (did) {
            debugger;
            var allocationId = did;
            var url = '@Url.Action("ViewRejection", "OwnSources")';
            $.ajax({
                type: "POST",
                url: url,
                data: { id: allocationId },
                success: function (response) {
                    if (response[0] == "Success") {
                        $(".modal-body #RejectionReason").val(response[1]);
                        $("#ViewReasonModal").modal("show");  // show modal
                    }
                    else if (response[0] == "DbException") {
                        swal("An error has occured, contact system support" + response[1]);
                        $("#divLoader").hide();
                    }
                },
                error: function (xhr) {
                    console.log(xhr.responseText);
                    alert("An error has occured, contact system support");
                    $("#divLoader").hide();
                },
            })
        }


        if ('@TempData["Success"]' == 'Success') {
            swal("Imported Successfully!", { icon: 'success' })
                 .then(Ok=> {
                     window.location.href = '@Url.Action("CreateOwnSource", "OwnSources")';
                 });
        } else if ('@TempData["Success"]' == 'DbException') {

            swal("DbException.. Contact System Administrator..")

        };

    </script>
}



