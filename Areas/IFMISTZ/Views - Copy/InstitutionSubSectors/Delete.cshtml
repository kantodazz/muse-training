@model IFMIS.Areas.IFMISTZ.Models.InstitutionSubSector

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="content" style="margin: 5px; padding-top: 50px">

    <section id="widget-grid" class="">
        <div class="row">

            <article class="col-sm-12 col-md-12 col-lg-12">

                <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false" data-widget-custombutton="false" data-widget-colorbutton="false" data-widget-deletebutton="false" data-widget-togglebutton="false">
                    <header>
                        <span class="widget-icon"> <i class="fa fa-edit"></i> </span>

                        <h2 style="color:black;"> Delete Institution Sub Sector.</h2>
                    </header>

                    <div>

                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.InstitutionSector.SectorName)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.InstitutionSector.SectorName)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.SubSectorName)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.SubSectorName)
                            </dd>

                        </dl>

                        @using (Html.BeginForm())
                        {
                            @Html.AntiForgeryToken()
                            <div class="form-actions">
                                <div class="row">
                                    <div class="col-md-6">
                                        <a href='@Url.Action("CreateSubSector", "InstitutionSubSectors")' class="btn btn-info" style="   width:80px;border-radius:1px;color:black">
                                            <i class="glyphicon glyphicon-arrow-left"></i>
                                            Back
                                        </a>
                                        <button type="submit" class="btn action-btn btn btn-info">
                                            <i class="fa fa-save"></i>
                                            Delete
                                        </button>
                                    </div>
                                </div>
                            </div>
                        }

                    </div>
                </div>
            </article>
        </div>
    </section>
</div>