            function Approve(i, id) {
                debugger;
                if (i == 'x') i = id;
                swal({
                    title: "Are you sure?",
                    text: "This process cannot be undone",
                    buttons: [
                      'Yes',
                      'No'
                    ],
                    closeOnClickOutside: false,
                }).then(function (isConfirm) {
                    if (isConfirm) {
                        swal("Cancelled", "No change was made");
                    } else {

                        ApproveTransfer(i)
                    }
                });
                $('#confirmModal').modal('hide');
            };




            function printSlip(id) {
                debugger;
                swal('view slip');
                // paymentNumber = data[i]["PaymentNo"];
                //entityCode = paymentNumber.split("D")[0];


                ///window.open("../../Areas/IFMISTZ/Reports/PaymentSummaryRPT.aspx?inst-code=" + entityCode + "&&payment-number=" + paymentNumber);
            };


            function attachSlip(i) {
                debugger;
                var transferSummaryId = i;
                $('#transferSummaryid').val(transferSummaryId)
                $('#attachFileModal').modal('show');
            };


            $("#saveLoader").toggle(false);
            function uploadFile() {
                debugger;
                swal({
                    text: "Confirm To Attach Slip ?",
                    buttons: ["Cancel", "Confirm"],
                }).then((confirmed) => {
                    if (confirmed) {
                        uploadFilesConfirmed();
                    } else {
                        swal("Cancelled");
                    }
                });
            };

            function uploadFilesConfirmed() {
                debugger;
                var formData = new FormData();
                var transferSummaryId = $('#transferSummaryid').val();
                formData.append('transferSummaryId', transferSummaryId);
                formData.append('file', $('input[type=file]')[0].files[0]);
                $("#form_submision").prop('disabled', true);
                $("#saveLoader").toggle(true);
                $.ajax({
                    url: '@Url.Action("AttachSlip", "FundTransfers")',
                    data: formData,
                    type: 'POST',
                    contentType: false,
                    processData: false,
                    success: function (response) {
                        $("#form_submision").prop('disabled', false);
                        $("#saveLoader").toggle(false);
                        if (response == "Success") {
                            swal("Slip Attached successfull!", { icon: "success" })
                              .then((e) => {
                                  $('#attachFileModal').modal('hide');
                                  window.location.reload();
                              });
                        } else {
                            swal(response)
                             .then((e) => {
                                 $('#attachFileModal').modal('hide');
                                 window.location.reload();
                             });
                        }

                    },
                    failure: function (error) {
                        $("#form_submision").prop('disabled', false);
                        $("#saveLoader").toggle(false);
                        swal(error)
                         .then((e) => {
                             $('#attachFileModal').modal('hide');
                             window.location.reload();
                         });
                    }
                });
            };



            var Print = function (id) {
                debugger;
                var Id = id;
                window.location.href = '@Url.Action("PrintTransfer", "FundTransfers")?transferSummaryId=' + Id;
            };



            //----Display Transfer Details---//
            var transferDetails = function (id) {
                debugger
                var url = '@Url.Action("TransferDetails", "FundTransfers")';
                $("#transferDetail_body").load(url, { id: id }, function (response, status, xhr) {
                    if (status == "error") {
                        $("#transferDetail_body").html(status + " Error on accessing this details, contact system support.");
                    }
                    $("#detailsModal").modal("show");
                });
            };
            //function to reset bootstrap modal popups
            $("#detailsModal").on("hidden.bs.modal", function () {
                $(".modal-header").removeClass("alert-danger").addClass("alert-info");
                $("#detailsModal").html("");
            });
            //--End Display Transfer Details---//

            $("#btncancel").click(function (e) {
                window.location.reload();
            });



            function reject(i, id) {
                debugger;
                swal({
                    title: "Rejection Reason",
                    closeOnClickOutside: false,
                    content: "input",
                    buttons: [
                      'Cancel',
                      'Save'
                    ],
                }).then((value) => {
                    //'
                    debugger;
                    var inputValue = `${value}`;
                    if (inputValue == "null") {
                        swal("Cancelled");

                    }
                    else {
                        rejectReasons(i, `${value}`)
                    }

                });
            }

            var rejectReasons = function (id, reason) {
                debugger;
                var rejectionReason = reason;

                $("#divLoader").show();

                $.ajax({
                    type: "POST",
                    url: "RejectTransfer",
                    data: { ids: id, reason: rejectionReason },
                    success: function (response) {
                        if (response == "Success") {
                            swal("Rejection Successfully Saved", { icon: "success" }).then((e) => {
                                window.location.reload();
                            });
                            //location.hide();
                            $("#divLoader").hide();

                        } else if (response == "DbException") {
                            swal(response + "An error has occured, contact system support");
                            $("#divLoader").hide();
                        }
                    },
                    error: function (xhr) {
                        //console.log(xhr.responseText);
                        alert("An error has occured, contact system support");
                        $("#divLoader").hide();
                    },
                });

            };


            function preview(slipfile) {
                debugger;
                var url = '@Url.Content("~/Media/TransferSlips/")'+ slipfile;

                document.getElementById("slipImage").src = url;
                $('#detailID').dialog({
                    autoOpen: true,
                    width: 900,
                    height: 700,
                    position: 'center',
                    resizable: true,
                    draggable: true
                });
            };

        </script>
    }
