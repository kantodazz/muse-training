                        <!-- widget edit box -->
                        <div class="jarviswidget-editbox">
                            <!-- This area used as dropdown edit box -->

                        </div>
                        <!-- end widget edit box -->
                        <!-- widget content -->
                        <div class="widget-body">



                            <table id="dt_basic" class="table table-bordered table-hover table-condensed" width="100%">
                                <thead>
                                    <tr style="text-align:right;white-space:nowrap">
                                        <th></th>
                                        <th data-hide="phone">Transfer Type</th>
                                        <th style="text-align:left">Transfer Category</th>
                                        <th data-class="expand" style="text-align:left"> Institution From </th>
                                        <th data-class="expand" style="text-align:left"> Institution To </th>
                                        <th data-class="expand" style="text-align:right"> Transfered Amount </th>

                                        <th data-class="expand" style="text-align:left">Transfer Date</th>
                                        <th data-hide="phone,tablet" style="text-align:center"> OverallStatus </th>
                                        <th></th>
                                        <th data-hide="phone,tablet"> Action</th>
                                    </tr>
                                </thead>
                                @foreach (var group in ((IEnumerable<IFMIS.Areas.IFMISTZ.Models.FundTransferSummary>)ViewBag.transferDetails).GroupBy(a => a.InstitutionCodeFrom))
                                {
                                    int i = 0;
                                    <tbody>
                                        @foreach (var item in group.Where(a=>a.TransferSlipFilePath == null && a.TransferType != "Own Source" && (a.OverallStatus == "Confirmed" || a.ApprovalStatus == "Approved" )))
                                        {

                                            i++;
                                            <tr>
                                                <td style="text-align:right">@i</td>
                                                <td>@item.TransferType</td>
                                                <td>@item.TransferCategory</td>
                                                <td style="text-align:left;white-space:nowrap">
                                                    @item.InstitutionCodeFrom - @item.InstitutionNameFrom
                                                </td>
                                                <td style="text-align:left">@item.InstitutionCodeTo - @item.InstitutionNameTo</td>
                                                <td style="text-align:right">@string.Format("{0:#,0.00}", item.TotalBaseAmount)</td>
                                                <td style="text-align:center">@string.Format("{0:d/M/yyyy}", item.CreatedAt)</td>

                                                <td style="text-align:center">@item.OverallStatus</td>
                                                <td> 
                                                    @if (item.TransferSlipFilePath != null)
                                                    {
                                                        <a href="#" onclick=preview("@item.TransferSlipFilePath")><i> Preview Slip </i></a>
                                                    }
                                                    else
                                                    {
                                                        <p>No Slip Attached </p>
                                                    }
                                                </td>
                                                <td>
                                                    <div class="btn-group">
                                                        
                                                        <button type="button" class="btn btn-info btn-xs dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                            <span class="caret"></span>
                                                            <span class="sr-only">Toggle Dropdown</span>
                                                        </button>
                                                        <ul class="dropdown-menu">
                                                            @if (@item.OverallStatus == "Confirmed")
                                                            {
                                                                <li><a href="#" onclick="Approve(@item.FundTransferSummaryId)"><i class="fa fa-edit"></i> Approve</a></li>
                                                                <li><a href="#" onclick="reject(@item.FundTransferSummaryId)"><i class="fa fa-trash-o"></i> Reject </a></li>
                                                            }
                                                            @if (@item.OverallStatus == "Approved" && item.TransferCategory == "Bank")
                                                            {
                                                                <li><a href="#" onclick="Print(@item.FundTransferSummaryId)"><i class="fa fa-print"></i> Print Slip </a></li>
                                                                <li><a href="#" onclick="attachSlip(@item.FundTransferSummaryId)"><i class="fa fa-paperclip"></i>   Attach Slip </a></li>
                                                            }

                                                            <li><a href="#" onclick="transferDetails(@item.FundTransferSummaryId)"><i class="fa fa-list"></i>   Details </a></li>

                                                        </ul>
                                                    </div>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                }
                                </table>
                                

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
