            var ApproveTransfer = function (id) {
                debugger;
                $("#divLoader").show();

                $.ajax({
                    type: "POST",
                    url: "ApproveTransfer",
                    data: { ids: id },
                    success: function (response) {
                        if (response == "Success") {
                            swal("Approved Successfully!", { icon: "success" }).then((e) => {
                                window.location.reload();
                            });
                            $("#divLoader").hide();

                        } else if (response == "DbException") {
                            alert("An error has occured, contact system support");
                            $("#divLoader").hide();
                        }
                        else
                        {
                            alert(response);
                            $("#divLoader").hide();
                        }
                    },
                    error: function (xhr) {
                        //console.log(xhr.responseText);
                        alert("An error has occured, contact system support");
                        $("#divLoader").hide();
                    },
                });


            };


