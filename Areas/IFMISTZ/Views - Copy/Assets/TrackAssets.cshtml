        $(function () { // this will be called when the DOM is ready
            $("#AssetName").keyup(function () {
                var query = $(this).val();
                var length = query.length;
                if (length > 1) {
                    get_items(query);
                }

            });
        });
         function get_items(query) {
            var form_data = {
                "Prefix": query
            }
            var url = '@Url.Action("SearchItems", "Assets")';
            $.ajax({
                type: "POST",
                url: url,
                data: form_data,
                success: function (data) {
                    if (data.response == "Success") {
                        var items_array = [];
                       $.each(data.Items, function (i, d) {
                           items_array.push(d.ItemDescription);
                        });


                        $('#AssetName').autocomplete({
                           source: items_array
                           });
                    }
                    else {

                    }

                }
            });
        }
        //SHOW LOADER ICON
        $(document).ajaxStart(function () {
            $(".loadingImg").show();

        });
        //HIDE LOADER ICON
        $(document).ajaxStop(function () {
            $(".loadingImg").hide();
        });
    </script>
