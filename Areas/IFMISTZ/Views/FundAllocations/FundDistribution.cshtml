                                                            <li><a href="#" onclick="editing(@group1.FundDistributionSummaryId)"><i class="fa fa-edit"></i> Edit</a></li>
                                                            <li><a href="../FundAllocations/Details/@group1.FundDistributionSummaryId"><i class="fa fa-search-plus"></i> Details </a></li>
                                                            <li><a href="#" onclick="Delete('x',@group1.FundDistributionSummaryId)"><i class="fa fa-trash-o"></i> Cancel </a></li>
                                                            @if (group1.OverallStatus == "Rejected")
                                                            {
                                                                <li><a href="#" onclick="viewReason(@group1.FundDistributionSummaryId)"><i class="fa fa-list"></i> View Reason </a></li>
                                                            }
                                                           
                                                        </ul>
                                                    </div>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
}
                                </table>
                            

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>

