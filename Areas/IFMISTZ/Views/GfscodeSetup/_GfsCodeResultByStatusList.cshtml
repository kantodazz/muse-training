@model IEnumerable<IFMIS.Areas.IFMISTZ.Models.GfscodeSetupDto>
    <table id="dt_basic" class="table table-striped table-bordered table-hover table-condensed" width="100%">
        <thead>
            <tr style="background-color: #f5f5f5; color:black">
                <th>#</th>
                <th>Gfs Code Name</th>
                <th>Sub Item</th>
                <th>Chapter Desc</th>
                <th>SubChapter Desc</th>
                <th>Item Name</th>
                <th>Item Combine</th>
                <th>Status</th>
            </tr>
        </thead>
        @{int i = 0;}
        @foreach (var item in Model)
        {
            i++;
            <tr>
                <td>@i</td>
                <td>@Html.DisplayFor(modelItem => item.GfscodeName) : @Html.DisplayFor(modelItem => item.GfscodeDesc)</td>
                <td>@Html.DisplayFor(modelItem => item.SubItemNo)</td>
                <td>@Html.DisplayFor(modelItem => item.ChapterCombineNo): @Html.DisplayFor(modelItem => item.ChapterName)</td>
                <td>@Html.DisplayFor(modelItem => item.SubChapterCombineNo): @Html.DisplayFor(modelItem => item.SubChapterName)</td>
                <td>@Html.DisplayFor(modelItem => item.GfscodeItemName)</td>
                <td>@Html.DisplayFor(modelItem => item.GfscodeItemCombinedNo) : @Html.DisplayFor(modelItem => item.GfscodeItemName)</td>
                <td>@Html.DisplayFor(modelItem => item.StatusLevel)</td>

            </tr>
        }
        </table>


<script>
    $(function () {
        $("#dt_basic").DataTable({

        })
    })
</script>
