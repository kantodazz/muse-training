@model IEnumerable<IFMIS.Areas.IFMISTZ.Models.FundDistributionSummary>
@if(ViewBag.withdraws != null){
     foreach (var group in ((IEnumerable<IFMIS.Areas.IFMISTZ.Models.FundDistributionSummary>)ViewBag.withdraws).GroupBy(a => a.InstitutionCode))
          {
    <table id="dt_basic" class="table table-striped table-bordered table-hover table-condensed" width="100%">
        <thead>
            <tr>
                <th> Funding Source </th>
                <th> Funding Type </th>
                <th> Sub Budget Class </th>
                <th> SubVote </th>
                <th style="text-align:right"> Distributed Amount </th>
                <th> Date </th>
                <th> OverallStatus </th>
                <th colspan="2"></th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in group)
                        {
                <tr>
                    <td>@item.FundingSource</td>
                    <td>@item.FundType</td>
                    <td>@item.BudgetClass</td>
                    <td>@item.SubVoteCode - @item.SubVoteCodeDesc </td>
                    <td style="text-align:right">@string.Format("{0:#,0.00}", item.TotalAmount) </td>
                    <td>@string.Format("{0:d/M/yyyy}", item.CreatedAt)</td>
                    <td>@item.OverallStatus </td>
                    <td>
                        <div class="btn-group">
                            <button type="button" class="btn btn-info btn-xs dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span class="caret"></span>
                                <span class="sr-only">Toggle Dropdown</span>
                            </button>
                            <ul class="dropdown-menu">
                                <li><a href="#" onclick="withdraws(@item.FundDistributionSummaryId)"> WithDraw </a></li>
                                <li><a href="../FundAllocations/Details/@item.FundDistributionSummaryId"> Details </a></li>
                                
                            </ul>
                        </div>
                    </td>
                </tr>
                      }
        </tbody>
    </table>
        }
}
else{
<p> No matching Result for Provided Distribution Reference Number  </p>
}
