@model IEnumerable<IFMIS.Areas.Gacs.Models.GlobalPeriod>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fiscal_year)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.period_description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.period_start_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.period_end_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.closed_flag)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fiscal_year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.period_description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.period_start_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.period_end_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.closed_flag)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
