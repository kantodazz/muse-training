            } else if (paymentMode == '') {
                swal("Payment Mode is Required")
                    .then((j) => {
                        $("#paymentMode").focus();
                        $("#paymentMode").css({ 'border-color': 'red' });
                    });

            } else if (payerAccountNumber == '') {
                swal("Payer Account Number is Required")
                    .then((j) => {
                        $("#payerAccountNumber").focus();
                        $("#payerAccountNumber").css({ 'border-color': 'red' });
                    });

            } else if (loanCharges == '') {
                swal("Loan Charges is Required")
                    .then((j) => {
                        $("#LoanCharges").focus();
                        $("#LoanCharges").css({ 'border-color': 'red' });
                    });
            }
            else {

                $("#AgencyCode").css({ 'border-color': '' });
                $("#AgencyDescription").css({ 'border-color': '' });
                $("#payerAccountNumber").css({ 'border-color': '' });
                $("#SubBudgetClass").css({ 'border-color': '' });
                $("#CashAccount").css({ 'border-color': '' });
                $("#GlExpenditureAccount").css({ 'border-color': '' });
                $("#GlPayableAccount").css({ 'border-color': '' });
                $("#percentage").css({ 'border-color': '' });
                $("#paymentMode").css({ 'border-color': '' });
                $("#LoanCharges").css({ 'border-color': '' });

                var form_data = {

                    "AgencyCode": $("#AgencyCode").val(),
                    "AgencyDesc": $("#AgencyDescription").val(),
                    "SubBudgetClass": $("#SubBudgetClass").val(),
                    "CashAccount": $("#CashAccount").val(),
                    "ExpenditureGLAccount": $("#GlExpenditureAccount").val(),
                    "PayableGLAccount": $("#GlPayableAccount").val(),
                    "PayeeDetailId": row[0],
                    "PayeeBIC": row[1],
                    "PayeeName": row[2],
                    "PayeeCode": row[3],
                    "AgencyTinNo": row[3],
                    "PayeeBankName": row[4],
                    "PayeeBankAccount": row[5],
                    "PayeeType":row[6],
                    "PaymentMode": $("#paymentMode").val(),
                    "AgencyFeeId": $("#LoanCharges").val(),
                    "PercentageFee": $("#percentage").val(),
                    "officeTel": $("#officeTel").val(),
                    "AgencyAddress": $("#Address").val(),
                    "AgencyRegion": $("#Region").val(),
                    "RequireControlNo": $("#_IsRequireCtrlNo").val(),
                    "PayerAccountNum": $("#payerAccountNumber").val(),
                    //"PayerAccountName": $("#payerBankname").val(),
                }

                var url = '@Url.Action("CreateFinancialInstitution", "FinancialInstitution")';
                var urlList = '@Url.Action("FinancialInstitutionList", "FinancialInstitution")';

                $.ajax({
                    type: "post",
                    url: url,
                    data: form_data,
                    data: { vm: form_data },
                    contenttype: "application/json; charset=utf-8",
                    datatype: "json",
                    success: function (response) {

                        if (response == "Success") {
                            swal("Saved Successfully!", { icon: "success" })
                                .then((value) => {
                                    window.location.href = urlList;
                                });
                        } else {
                            swal(response, { icon: "warning" })
                                .then((value) => {
                                    window.location.href = urlList;
                                });
                        }

                    },
                    failure: function (error) {

                        swal(error);
                    }
                });
            }

        });




    </script>
}


