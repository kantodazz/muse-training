@model IEnumerable<IFMIS.Areas.ALS.Models.ALS_FundWithDrawSummary>
    <table id="dt_basic" class="table table-striped table-bordered table-hover table-condensed" width="100%">
        <thead>
            <tr style="background-color: #f5f5f5; color:black">
                <th>#</th>
                <th>Funding Ref No.</th>
                @*<th>Allocation Ref No.</th>*@
                <th>Funding Source</th>
                @*<th>Allocated Vote Detail</th>*@
                <th>Total Withdraw Amount(TZs)</th>
                <th>Status</th>
                <th style="width:30px">Actions</th>
            </tr>
        </thead>
        @{int i = 0;}
        @foreach (var item in Model)
        {
            i++;
            <tr>
                <td>@i</td>
                <td>@Html.DisplayFor(modelItem => item.FundingRefNo)</td>
                <td>@Html.DisplayFor(modelItem => item.FundingSource)</td>
                <td>@Html.DisplayFor(modelItem => item.TotalWithdrawAmount)</td>
                @*<td>@Html.DisplayFor(modelItem => item.ALS_FundAllocationDetails)</td>*@
                <td>@Html.DisplayFor(modelItem => item.OverallStatus)</td>
                @*<td>@Html.DisplayFor(modelItem => item.FundingRefNo)</td>*@
                <td></td>
            </tr>
        }
        </table>

<script>
    $(function () {
        $("#dt_basic").DataTable({

        })
    })
</script>
