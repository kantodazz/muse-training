@section pagespecific{
    <script>
        $('#Reschedule1').toggle(false)
        $('#Reschedule2').toggle(false)
        $('#Reschedule3').toggle(false)
        $('#Reschedule4').toggle(false)
        $('#auctioneer1').toggle(false)
        $('#auctioneer2').toggle(false)
        $('#auctioneer3').toggle(false)
        $('#auctioneer4').toggle(false)
        $('#auctioneer5').toggle(false)
        $('#debtCollector1').toggle(false)
        $('#case1').toggle(false)

        $('#Deseased').toggle(false)
        $('#Bankruptcy').toggle(false)
        $('#both1').toggle(false)
        $('#both2').toggle(false)
        $('#both3').toggle(false)

        $("#LifeStatus").change(function () {
            var lifestatus = $("#LifeStatus").val();
            if (lifestatus == "Deseased") {
                $('#Deseased').toggle(true)
                $('#both1').toggle(true)
                $('#both2').toggle(true)
                $('#both3').toggle(true)
                $('#Bankruptcy').toggle(false)
                $("#EstateAdministrator").val("")
                $("#Liquidator").val("")
                $("#Phone").val("")
                $("#Address").val("")

            } else if (lifestatus == "Bankruptcy") {
                $('#Bankruptcy').toggle(true)
                $('#both1').toggle(true)
                $('#both2').toggle(true)
                $('#both3').toggle(true)
                $('#Deseased').toggle(false)
                $("#EstateAdministrator").val("")
                $("#Liquidator").val("")
                $("#Phone").val("")
                $("#Address").val("")
            } else {
                $('#Deseased').toggle(false)
                $('#Bankruptcy').toggle(false)
                $('#both1').toggle(false)
                $('#both2').toggle(false)
                $('#both3').toggle(false)
                $("#EstateAdministrator").val("")
                $("#Liquidator").val("")
                $("#Phone").val("")
                $("#Address").val("")
            }
        });


        $("#Status").change(function () {
            var status = $("#Status").val();
            if (status == "Reschedule") {
                $('#Reschedule1').toggle(true)
                $('#Reschedule2').toggle(false)
                $('#Reschedule3').toggle(false)
                $('#Reschedule4').toggle(false)
                $('#auctioneer1').toggle(false)
                $('#auctioneer2').toggle(false)
                $('#auctioneer3').toggle(false)
                $('#auctioneer4').toggle(false)
                $('#auctioneer5').toggle(false)
                $('#debtCollector1').toggle(false)
                $('#case1').toggle(false)
            } else if (status == "Auction") {
                // $('#Reschedule2').toggle(true)
                $('#auctioneer1').toggle(true)
                $('#auctioneer2').toggle(true)
                $('#auctioneer3').toggle(true)
                $('#auctioneer4').toggle(true)
                $('#auctioneer5').toggle(true)
                $('#Reschedule1').toggle(false)
                $('#Reschedule2').toggle(false)
                $('#Reschedule3').toggle(false)
                $('#Reschedule4').toggle(false)
                $('#debtCollector1').toggle(false)
                $('#case1').toggle(false)
            } else if (status == "Debt Collector") {
                $('#debtCollector1').toggle(true)
                $('#auctioneer1').toggle(true)
                $('#auctioneer2').toggle(true)
                $('#auctioneer3').toggle(false)
                $('#auctioneer4').toggle(false)
                $('#auctioneer5').toggle(true)
                $('#Reschedule1').toggle(false)
                $('#Reschedule2').toggle(false)
                $('#Reschedule3').toggle(false)
                $('#Reschedule4').toggle(false)
            } else if (status == "Legal") {
                $('#Reschedule1').toggle(false)
                $('#Reschedule2').toggle(false)
                $('#Reschedule3').toggle(false)
                $('#Reschedule4').toggle(false)
                $('#auctioneer1').toggle(false)
                $('#auctioneer2').toggle(false)
                $('#auctioneer3').toggle(false)
                $('#auctioneer4').toggle(false)
                $('#auctioneer5').toggle(false)
                $('#debtCollector1').toggle(false)
                $('#case1').toggle(true)
             } else if (status == "Normal") {
                    $('#Reschedule1').toggle(false)
                    $('#Reschedule2').toggle(false)
                    $('#Reschedule3').toggle(false)
                    $('#Reschedule4').toggle(false)
                    $('#auctioneer1').toggle(false)
                    $('#auctioneer2').toggle(false)
