        //Save button click function
        $("#btnSave").click(function () {

            var form_data = {
                "LifeStatus": $("#LifeStatus").val(),
                "Status": $("#Status").val(),
                "StartDate": $("#StartDate").val(),
                "LoanReschedulingType": $("#ReschedulingTypeName").val(),
                "RepaymentPeriod": $("#RepaymentPeriod").val(),
                "RepaymentFrequency": $("#RepaymentFrequency").val(),
                "Reason": $("#Reason").val(),

                "EstateAdministrator": $("#EstateAdministrator").val(),
                "Liquidator": $("#Liquidator").val(),
                "Phone": $("#Phone").val(),
                "Address": $("#Address").val(),

                "LoanApplicationId": '@ViewBag.LoanApplication.LoanApplicationId',
                "ApplicationNo": '@ViewBag.LoanApplication.ApplicationNo',
                "LoanPurpose": '@ViewBag.LoanApplication.LoanPurpose',
                "LoanApprovedAmount": '@ViewBag.LoanApplication.LoanApprovedAmount',
                "LoanCustomerId": '@ViewBag.LoanApplication.LoanCustomerId',
                "CustomerName": '@ViewBag.LoanApplication.CustomerName'
            }
            console.log(form_data);
            var url = '@Url.Action("ChangeStatus", "LoanMonitoring")';
            var urlList = '@Url.Action("LoanMonitoringList", "LoanMonitoring")';
            $.ajax({
                type: "post",
                url: url,
                data: form_data,
                contenttype: "application/json; charset=utf-8",
                datatype: "json",
                success: function (response) {
                    if (response == "Success") {
                        swal("Saved Successfully!", { icon: "success" })
                            .then((value) => {
                                window.location.href = urlList;
                            });
                    }
                    else {
                        swal(response);
                    }
                },
                failure: function (error) {
                    swal(error);
                }
            });
        });

    </script>

