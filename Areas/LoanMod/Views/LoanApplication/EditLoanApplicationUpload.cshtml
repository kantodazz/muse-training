            {
                type: "POST",
                url: url,
                data: { id: id },
                success: function (resultData) {
                    $(".delete").hide();
                    if (resultData.response == "Success") {
                        swal("Attachment Cancelled Successfully!", { icon: "success" })
                            .then((m) => {
                                window.location.href ='@Request.Url.ToString()';
                            });
                    }
                },
                error: function (xhr) {
                    $(".delete").show();
                    swal("Error in Cancelling Attachment");
                },

            });
                    } else {
                        $(".delete").show();
