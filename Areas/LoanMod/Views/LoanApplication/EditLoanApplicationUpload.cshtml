                    text: "Do you what to Cancel Attachment " + name + " ?",
                    buttons: [
                      'No',
                      'Yes'
                    ],
                }).then(function (isConfirm) {
                    if (isConfirm) {
                        var url = '@Url.Action("DeleteLoanAttachment", "LoanApplication")';
